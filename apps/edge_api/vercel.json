{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ],
    "git": {
        "deploymentEnabled": {
            "main": true,
            "dev": false,
            "*-preview": true,
            "exp-*": false
        }
    },
    "outputDirectory": ".",
    "installCommand": "bun install"
}